{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "React Boilerplate",
  "main": "src/server.js",
  "repository": "wdelacruz.com",
  "author": "Wilowayne De La Cruz G. <wilo0087@gmail.com>",
  "license": "MIT Wilowayne De La Cruz",
  "private": true,
  "scripts": {
    "start": "yarn babel-node server.js",
    "start_mon": "nodemon --exec babel-node server.js",
    "build": "rm -rf ./public && yarn webpack --mode development --colors --config config/webpack.base.config.js --env.PLATFORM=developent --env.VERSION=stag --open --hot",
    "buildprod": "rm -rf ./public && yarn webpack --mode production --config config/webpack.prod.config.js --env.PLATFORM=production --env.VERSION=stag --progress",
    "watch": "rm -rf ./public && yarn webpack --progress --colors --watch --config config/webpack.base.config.js --env.PLATFORM=developent --env.VERSION=stag --open --hot",
    "test": "yarn jest"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "express": "^4.16.4",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.3.1",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "browser-sync": "^2.26.3",
    "browser-sync-webpack-plugin": "^2.2.2",
    "copy-webpack-plugin": "^5.0.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.3.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "react-test-renderer": "^16.8.4",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-merge": "^4.2.1",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
